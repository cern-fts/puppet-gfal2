# configuration file for http plugin
[HTTP PLUGIN]
ENABLE_REMOTE_COPY=<%= @enable_remote_copy %>

## Enable streamed copies (data passes via the client)
ENABLE_STREAM_COPY=<%= @enable_stream_copy %>

#Enable fallback copies: in case of TPC we start first with DEFAULT_COPY_MODE
#and we fallback in case of an error
ENABLE_FALLBACK_TPC_COPY=<%= @enable_fallback_tpc_copy %>

# Default mode for http copy
DEFAULT_COPY_MODE=<%= @default_copy_mode %>

# Enable or disable the SSL CA check
INSECURE=<%= @insecure %>

# Force Davix log level
LOG_LEVEL=<%= @log_level %>

# Use HTTP Keep-Alive
KEEP_ALIVE=<%= @keep_alive %>


# AWS S3 related options
[S3]

# AWS Secret key
SECRET_KEY=<%= @s3_secret_key %>

## AWS Access key
ACCESS_KEY=<%= @s3_access_key %>

# AWS Short lived token
TOKEN=<%= @s3_token %>

# AWS Region
REGION=<%= @s3_region %>

<% if @gcloud_json_auth_file  or @gcloud_json_auth_path -%>
#gcloud related options
[GCLOUD]

<% if @gcloud_json_auth_file -%>
# Path to the google JSON file path
JSON_AUTH_FILE=<%= @gcloud_json_auth_file %>
<% end -%>
<% if @gcloud_json_auth_path -%>
### google auth file as a string
JSON_AUTH_STRING=<%= @gcloud_json_auth_path %>
<% end -%>
<% end -%>
